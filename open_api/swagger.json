{
	"swagger":"2.0",
	"info":{
		"description":"This is a description of demo server API for [Catinder app](https://github.com/aleksey-paykachev/Catinder) in OpenAPI format.",
		"version":"v 1.0",
		"title":"Catinder OpenAPI"
	},
	"host":"raw.githubusercontent.com",
	"basePath":"/aleksey-paykachev/Catinder-Demo-Server/master/api/v1",
	"tags":[
		{
			"name":"Profiles",
			"description":"User profiles."
		},
		{
			"name":"Relationships",
			"description":"Relationships between users."
		},
		{
			"name":"Conversations",
			"description":"Conversation messages between users."
		}
	],
	"schemes":[
		"https"
	],
	"paths":{
		"/profiles":{
			"get":{
				"tags":[
					"Profiles"
				],
				"summary":"Get all profiles",
				"description":"Get all not matched (new) profiles for currently logged in user.",
				"operationId":"getAllProfiles",
				"produces":[
					"application/json"
				],
				"responses":{
					"200":{
						"description":"Successful operation",
						"schema":{
							"type":"array",
							"items":{
								"$ref":"#/definitions/Profile"
							}
						}
					}
				},
				"security":[
					{
						"catinder_auth":[
							"read:data"
						]
					}
				]
			}
		},
		"/profile/{profileUid}":{
			"get":{
				"tags":[
					"Profiles"
				],
				"summary":"Get profile by uid",
				"description":"Get full profile user data description of given user's uid.",
				"operationId":"getProfileByUid",
				"produces":[
					"application/json"
				],
				"parameters":[
					{
						"name":"profileUid",
						"in":"path",
						"description":"Uid of profile to return",
						"required":true,
						"type":"string"
					}
				],
				"responses":{
					"200":{
						"description":"Successful operation",
						"schema":{
							"$ref":"#/definitions/Profile"
						}
					},
					"400":{
						"description":"Invalid uid supplied"
					},
					"404":{
						"description":"Profile for given uid not found"
					}
				},
				"security":[
					{
						"catinder_auth":[
							"read:data"
						]
					}
				]
			}
		},
		"/matches":{
			"get":{
				"tags":[
					"Relationships"
				],
				"summary":"Get all matches",
				"description":"Get all matched profiles for currently logged in user.",
				"operationId":"getAllMatches",
				"produces":[
					"application/json"
				],
				"responses":{
					"200":{
						"description":"Successful operation",
						"schema":{
							"type":"array",
							"items":{
								"$ref":"#/definitions/Match"
							}
						}
					}
				},
				"security":[
					{
						"catinder_auth":[
							"read:data"
						]
					}
				]
			}
		},
		"/relationships/set-like":{
			"post":{
				"tags":[
					"Relationships"
				],
				"summary":"Set like",
				"description":"Set like for provided profile's uid related to currently logged in user, and test for like mutuality.",
				"operationId":"setLike",
				"consumes":[
					"application/json"
				],
				"produces":[
					"application/json"
				],
				"parameters":[
					{
						"name":"profileUid",
						"in":"formData",
						"description":"Uid of profile to set like to",
						"required":true,
						"type":"string"
					}
				],
				"responses":{
					"200":{
						"description":"successful operation",
						"schema":{
							"$ref":"#/definitions/SetLikeResponse"
						}
					},
					"400":{
						"description":"Invalid uid supplied"
					},
					"404":{
						"description":"Profile for given uid not found"
					}
				},
				"security":[
					{
						"catinder_auth":[
							"write:data",
							"read:data"
						]
					}
				]
			}
		},
		"/relationships/set-dislike":{
			"post":{
				"tags":[
					"Relationships"
				],
				"summary":"Set dislike",
				"description":"Set dislike for provided profile's uid related to currently logged in user.",
				"operationId":"setDislike",
				"consumes":[
					"application/json"
				],
				"produces":[
					"application/json"
				],
				"parameters":[
					{
						"name":"profileUid",
						"in":"formData",
						"description":"Uid of profile to set dislike to",
						"required":true,
						"type":"string"
					}
				],
				"responses":{
					"200":{
						"description":"successful operation",
						"schema":{
							"$ref":"#/definitions/SetDislikeResponse"
						}
					},
					"400":{
						"description":"Invalid uid supplied"
					},
					"404":{
						"description":"Profile for given uid not found"
					}
				},
				"security":[
					{
						"catinder_auth":[
							"write:data",
							"read:data"
						]
					}
				]
			}
		},
		"/messages/{profileUid}":{
			"get":{
				"tags":[
					"Conversations"
				],
				"summary":"Get messages",
				"description":"Get all messages for conversation between given profile's uid and currently logged in user.",
				"operationId":"getAllMessages",
				"produces":[
					"application/json"
				],
				"parameters":[
					{
						"name":"profileUid",
						"in":"path",
						"description":"Uid of collocutor profile to get messages for",
						"required":true,
						"type":"string"
					}
				],
				"responses":{
					"200":{
						"description":"Successful operation",
						"schema":{
							"type":"array",
							"items":{
								"$ref":"#/definitions/Message"
							}
						}
					},
					"400":{
						"description":"Invalid uid supplied"
					},
					"404":{
						"description":"Profile for given uid not found or haven't matched with current user yet"
					}
				},
				"security":[
					{
						"catinder_auth":[
							"read:data"
						]
					}
				]
			}
		},
		"/messages/{profileUid}/send":{
			"post":{
				"tags":[
					"Conversations"
				],
				"summary":"Send message",
				"description":"Send message from currently logged in user to provided profile's uid.",
				"operationId":"sendMessage",
				"consumes":[
					"application/json"
				],
				"produces":[
					"application/json"
				],
				"parameters":[
					{
						"name":"profileUid",
						"in":"path",
						"description":"Uid of collocutor profile to send messages to",
						"required":true,
						"type":"string"
					},
					{
						"name":"message",
						"in":"formData",
						"description":"Sended message body",
						"required":true,
						"type":"object",
						"schema":{
							"$ref":"#/definitions/Message"
						}
					}
				],
				"responses":{
					"200":{
						"description":"successful operation",
						"schema":{
							"$ref":"#/definitions/SendMessageResponse"
						}
					},
					"400":{
						"description":"Invalid uid supplied"
					},
					"404":{
						"description":"Profile for given uid not found or haven't matched with current user yet"
					}
				},
				"security":[
					{
						"catinder_auth":[
							"write:data",
							"read:data"
						]
					}
				]
			}
		},
	},
	"securityDefinitions":{
		"catinder_auth":{
			"type":"oauth2",
			"authorizationUrl":"https://catinder-fake-url.ru/oauth/authorize",
			"flow":"implicit",
			"scopes":{
				"read:data":"read data",
				"write:data":"modify data related to current account"
			}
		}
	},
	"definitions":{
		"Profile":{
			"type":"object",
			"properties":{
				"uid":{
					"type":"string",
					"example":"E17D64B9-7604-4069-9B8C-B1ED37219F1B"
				},
				"name":{
					"type":"string",
					"example":"Пушистик"
				},
				"age":{
					"type":"integer",
					"format":"int32",
					"example":3
				},
				"photosNames":{
					"type":"array",
					"items":{
						"type":"string",
						"example": "Fluffy_1.jpg"
					}
				},
				"shortDescription":{
					"type":"string",
					"example":"Люблю, когда меня гладят."
				},
				"extendedDescription":{
					"type":"string",
					"example":"Погладь меня, я весь чешуся."
				}
			}
		},
		"Message":{
			"type":"object",
			"properties":{
				"date":{
					"type":"string",
					"format":"date-time",
					"example":"2020-04-22T12:19:22+03:00"
				},
				"senderUid":{
					"type":"string",
					"example":"E17D64B9-7604-4069-9B8C-B1ED37219F1B"
				},
				"receiverUid":{
					"type":"string",
					"example":"195B63B8-5361-4014-8285-81D3763957ED"
				},
				"text":{
					"type":"string",
					"example":"Привет. Как у тебя дела?"
				}
			}
		},
		"Match":{
			"type":"object",
			"properties":{
				"matchDate":{
					"type":"string",
					"format":"date-time",
					"example":"2020-04-21T18:25:43+03:00"
				},
				"profile1":{
					"type":"object",
					"$ref":"#/definitions/Profile"
				},
				"profile2":{
					"type":"object",
					"$ref":"#/definitions/Profile"
				},
				"lastMessage":{
					"type":"object",
					"$ref":"#/definitions/Message"
				}
			}
		},
		"SetLikeResponse":{
			"type":"object",
			"properties":{
				"result":{
					"type":"string",
					"example":"success"
				},
				"isLikeMutual":{
					"type":"boolean",
					"example":true
				}
			}
		},
		"SetDislikeResponse":{
			"type":"object",
			"properties":{
				"result":{
					"type":"string",
					"example":"success"
				}
			}
		},
		"SendMessageResponse":{
			"type":"object",
			"properties":{
				"result":{
					"type":"string",
					"example":"success"
				}
			}
		}
	}
}